	"scripts": {
		"start": "webpack-dev-server --history-api-fallback --config webpack.dev.js",
		"start:docker": "npm run start --host 0.0.0.0",
		"build": "npm run clean && webpack --config webpack.prod.js",
		"build:staging": "npm run clean && webpack --config webpack.staging.js",
		"build:serve": "npm run clean && npm run build && http-serve ./build --gzip -f /",
		"profile": "webpack --watch --profile --json > compilation-stats.json --config webpack.prod.js",
		"clean": "rimraf ./build",
		"lint": "prettier --write \"src/**/*.js\" && eslint src/",
		"lint:watch": "esw src/ -w",
		"test": "jest",
		"test:watch": "jest --watch --coverage",
		"test:coverage": "jest --coverage",
		"test:ci": "npm run lint && jest --config=\"./config/tests/jest.config.json\""
	},
	"engines": {
		"node": ">= 8.9.1",
		"npm": ">= 5.6.0"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>/config/tests/styleMock.js",
			"^~/(.*)": "<rootDir>/src/$1"
		},
		"setupFilesAfterEnv": [
			"./node_modules/jest-enzyme/lib/index.js"
		],
		"testEnvironment": "enzyme",
		"testEnvironmentOptions": {
			"enzymeAdapter": "react16"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/e2e/"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/config/",
			"/src/index",
			"/src/infrastructure/reducers/index",
			"/src/infrastructure/getStore",
			"/src/infrastructure/icons",
			"/src/infrastructure/initSagas",
			"/src/infrastructure/network",
			"/src/utility/getQuery",
			"/src/utility/history",
			"/src/testhelpers/",
			"/e2e/"
		],
		"testURL": "http://localhost/",
		"coverageReporters": [
			"json",
			"html",
			"text"
		]
	},
	"browserslist": [
		"> 0.25%",
		"not dead"
	]
}
