	"scripts": {
		"dev": "webpack -d --progress --watch --profile --json > compilation-stats.json --env.dev",
		"start": "webpack-dev-server --progress --colors --inline --env.dev --history-api-fallback",
		"build": "webpack -p --progress --env.prod",
		"build:dev": "webpack -p --progress --env.dev",
		"clean": "rimraf ./build",
		"lint": "eslint src/",
		"lint:watch": "esw src/ -w",
		"generate-mock-data": "babel-node ./api/generateMockData.js",
		"prestart-mockapi": "npm run generate-mock-data",
		"start-mockapi": "json-server --watch api/db.json --port 3001",
		"test": "jest",
		"test:watch": "jest --watch --coverage",
		"test:coverage": "jest --coverage",
		"test:ci": "jest --config=\"./config/tests/jest.config.json\" && codecov",
		"test:e2e": "cross-env JEST_PUPPETEER_CONFIG=\"config/tests/puppeteer.config.js\" jest --config=\"./config/tests/jest.e2e.config.json\" --runInBand"
	},
	"engines": {
		"node": ">= 6.0.0",
		"npm": ">= 5.0.0"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>/config/tests/styleMock.js"
		},
		"setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
		"testEnvironment": "enzyme",
		"testEnvironmentOptions": {
			"enzymeAdapter": "react15"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/e2e/"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/config/",
			"/src/index",
			"/src/utility/getQuery",
			"/src/utility/history",
			"/e2e/"
		],
		"testURL": "http://localhost/"
	}
}
